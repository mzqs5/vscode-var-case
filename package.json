{
	"name": "mzqs5",
	"displayName": "Var Case",
	"description": "Variable first letter case conversion",
	"publisher": "mzqs5",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzqs5/vscode-var-case"
  },
  "icon": "icon.png",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.toVarLowerCase",
		"onCommand:extension.toVarUpperCase"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.toVarLowerCase",
				"title": "首字母小写"
			},
			{
				"command": "extension.toVarUpperCase",
				"title": "首字母大写"
			}
		],
		"keybindings": [
			{
			  "command": "extension.toVarLowerCase",
			  "key": "alt+i",
			  "when": "editorHasSelection"
			},
			{
				"command": "extension.toVarUpperCase",
				"key": "alt+u",
				"when": "editorHasSelection"
			  }
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
